<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Phone_validation</fullName>
    <active>true</active>
    <errorConditionFormula>NOT( REGEX( Contact__c , &quot;^[0-9]{10}$&quot;) )</errorConditionFormula>
    <errorDisplayField>Contact__c</errorDisplayField>
    <errorMessage>Enter valid contact number</errorMessage>
</ValidationRule>
